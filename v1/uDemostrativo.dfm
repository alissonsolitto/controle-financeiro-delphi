  object qryTotal: TADOQuery
    Connection = DM.ADOConnection
    CursorType = ctStatic
    CommandTimeout = 0
    Parameters = <>
    SQL.Strings = (
      'SELECT R.Janeiro - D.Janeiro Janeiro,'
      #9#9'   R.Fevereiro - D.Fevereiro Fevereiro,'
      #9'     R.Marco - D.Marco Marco,'
      #9#9'   R.Abril - D.Abril Abril,'
      #9#9'   R.Maio - D.Maio Maio,'
      '       R.Junho - D.Junho Junho,'
      '       R.Julho - D.Julho Julho,'
      '       R.Agosto - D.Agosto Agosto,'
      '       R.Setembro - D.Setembro Setembro,'
      '       R.Outubro - D.Outubro Outubro,'
      '       R.Novembro - D.Novembro Novembro,'
      '       R.Dezembro - D.Dezembro Dezembro,'
      ''
      '      ((R.Janeiro - D.Janeiro )+'
      #9'    (R.Fevereiro - D.Fevereiro )+'
      '      (R.Marco - D.Marco )+'
      #9'    (R.Abril - D.Abril )+'
      '      (R.Maio - D.Maio )+'
      '      (R.Junho - D.Junho )+'
      '      (R.Julho - D.Julho )+'
      '      (R.Agosto - D.Agosto )+'
      '      (R.Setembro - D.Setembro )+'
      '      (R.Outubro - D.Outubro )+'
      '      (R.Novembro - D.Novembro )+'
      '      (R.Dezembro - D.Dezembro )) AS VL_Saldo_Anual,'
      ''
      '       ((R.Janeiro - D.Janeiro )+'
      #9'    (R.Fevereiro - D.Fevereiro )+'
      '      (R.Marco - D.Marco )+'
      #9'    (R.Abril - D.Abril )+'
      '      (R.Maio - D.Maio )+'
      '      (R.Junho - D.Junho )+'
      '      (R.Julho - D.Julho )+'
      '      (R.Agosto - D.Agosto )+'
      '      (R.Setembro - D.Setembro )+'
      '      (R.Outubro - D.Outubro )+'
      '      (R.Novembro - D.Novembro )+'
      
        '      (R.Dezembro - D.Dezembro )) + Total_Geral.VL_Saldo_Total A' +
        'S VL_Saldo_Total'
      'FROM(SELECT'
      '    ISNULL(SUM(Janeiro     ), 0.0) Janeiro,'
      #9#9'ISNULL(SUM(Fevereiro   ), 0.0)Fevereiro,'
      #9#9'ISNULL(SUM(Marco       ), 0.0)Marco,'
      #9#9'ISNULL(SUM(Abril       ), 0.0)Abril,'
      #9#9'ISNULL(SUM(Maio        ), 0.0)Maio,'
      #9#9'ISNULL(SUM(Junho    ), 0.0)Junho,'
      #9#9'ISNULL(SUM(Julho    ), 0.0)Julho,'
      #9#9'ISNULL(SUM(Agosto   ), 0.0)Agosto,'
      #9#9'ISNULL(SUM(Setembro ), 0.0)Setembro,'
      #9#9'ISNULL(SUM(Outubro  ), 0.0)Outubro,'
      #9#9'ISNULL(SUM(Novembro ), 0.0)Novembro,'
      #9#9'ISNULL(SUM(Dezembro) , 0.0)Dezembro'
      #9'FROM DEMONSTRATIVO'
      #9'WHERE ST_Lancamento = '#39'R'#39
      '  AND ID_Categoria <> 0) R'
      'OUTER APPLY(SELECT'
      '        ISNULL(SUM(Janeiro     ), 0.0) Janeiro,'
      #9#9#9#9'ISNULL(SUM(Fevereiro   ), 0.0)Fevereiro,'
      #9#9#9#9'ISNULL(SUM(Marco       ), 0.0)Marco,'
      #9#9#9#9'ISNULL(SUM(Abril       ), 0.0)Abril,'
      #9#9#9#9'ISNULL(SUM(Maio        ), 0.0)Maio,'
      #9#9#9#9'ISNULL(SUM(Junho    ), 0.0)Junho,'
      #9#9#9#9'ISNULL(SUM(Julho    ), 0.0)Julho,'
      #9#9#9#9'ISNULL(SUM(Agosto   ), 0.0)Agosto,'
      #9#9#9#9'ISNULL(SUM(Setembro ), 0.0)Setembro,'
      #9#9#9#9'ISNULL(SUM(Outubro  ), 0.0)Outubro,'
      #9#9#9#9'ISNULL(SUM(Novembro ), 0.0)Novembro,'
      #9#9#9#9'ISNULL(SUM(Dezembro) , 0.0)Dezembro'
      #9#9#9'FROM DEMONSTRATIVO'
      #9#9#9'WHERE ST_Lancamento = '#39'D'#39
      '        AND ID_Categoria <> 0) D'
      'OUTER APPLY(SELECT SUM(VL_Saldo) VL_Saldo_Total'
      '            FROM CONTA) AS Total_Geral'
      ''
      '')
    Left = 952
    Top = 40
